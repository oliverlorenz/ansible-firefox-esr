{
  "policies": {
    "BlockAboutAddons": false,
    "BlockAboutSupport": true,
    "DefaultDownloadDirectory": "${home}/downloads",
    "DisableFirefoxAccounts": true,
    "DisableMasterPasswordCreation": true,
    "DisablePocket": true,
    "DisableProfileImport": true,
    "DisplayBookmarksToolbar": true,
    "DownloadDirectory": "${home}/downloads",
    "ExtensionUpdate": true,
    "Extensions": {
      "Install": [
        "https://addons.mozilla.org/firefox/downloads/latest/ublock-origin/latest.xpi",
        "https://addons.mozilla.org/firefox/downloads/latest/passff/latest.xpi",
        "https://addons.mozilla.org/firefox/downloads/latest/dustman/latest.xpi",
        "https://addons.mozilla.org/firefox/downloads/latest/enterprise-policy-generator/latest.xpi",
        "https://addons.mozilla.org/firefox/downloads/latest/send-to-xbmc-kodi/latest.xpi",
        "https://addons.mozilla.org/firefox/downloads/latest/containers-with-transitions/latest.xpi"
      ]
    },
    "FirefoxHome": {
      "Highlights": false,
      "Locked": true,
      "Pocket": false,
      "Search": true,
      "Snippets": false,
      "TopSites": false
    },
    "NewTabPage": true,
    "OfferToSaveLogins": false,
    "OfferToSaveLoginsDefault": false,
    "PasswordManagerEnabled": false,
    "Preferences": {
      "browser.urlbar.suggest.bookmark": true,
      "browser.urlbar.suggest.history": false,
      "browser.urlbar.suggest.openpage": true,
      "media.gmp-gmpopenh264.enabled": true,
      "media.gmp-widevinecdm.enabled": true
    },
    "SearchSuggestEnabled": false
  }
}